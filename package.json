    "morgan": "^1.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-emotion": "^9.2.12",
    "react-hot-loader": "^4.3.12",
    "react-svg": "^6.0.24",
